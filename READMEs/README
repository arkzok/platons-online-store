# Activating virtual environment in  Powershell
venv/scripts/activate

# Activating virtual environment on Pythonanywhere
source .virtualenvs/arkzok.pythonanywhere.com/bin/activate

# Switching to the project folder
cd store_project

# Switching to main folder on Pythonanywhere
cd arkzok.pythonanywhere.com

# When changing the database structure
python manage.py makemigrations
python manage.py migrate

# Starting the server
python manage.py runserver